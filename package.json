		"lint-fix": "./node_modules/.bin/eslint --fix ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/carriejv/nvp-json-js.git"
	},
	"bugs": {
		"url": "https://github.com/carriejv/nvp-json-js/issues"
	},
	"homepage": "https://github.com/carriejv/nvp-json-js",
	"author": "Carrie Vrtis <carrie@carriejv.com> (https://www.carrievrtis.com)",
	"contributors": [
		"Carrie Vrtis <carrie@carriejv.com> (https://www.carrievrtis.com)"
	],
	"keywords": [
		"paypal",
		"payflow",
		"payflow",
		"pro",
		"nvp",
		"json",
		"api",
		"parser"
	],
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"chai": "^4.2.0",
		"codacy-coverage": "^3.4.0",
		"eslint": "^5.15.3",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-node": "^8.0.1",
		"eslint-plugin-promise": "^4.0.1",
		"eslint-plugin-standard": "^4.0.0",
		"istanbul": "^0.4.5",
		"mocha": "^5.2.0",
		"mocha-lcov-reporter": "^1.3.0"
	}
